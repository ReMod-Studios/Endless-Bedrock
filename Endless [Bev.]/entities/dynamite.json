{
	"format_version": "1.16.100",
	"minecraft:entity": {
		"description": {
			"identifier": "endless:dynamite",
			"is_spawnable": false,
			"is_summonable": true,
			"is_experimental": false,
			"runtime_identifier": "minecraft:snowball"
		},
		"components": {
			"minecraft:collision_box": {
				"width": 0.25,
				"height": 0.25
			},
			"minecraft:projectile": {
				"on_hit": {
					"impact_damage": {
						"damage": 4,
						"knockback": true
					},
					"stick_in_ground": {},
					"definition_event": {
						"event_trigger": {
							"event": "endless:post_hit"
						}
					}
				},
				"anchor": 1,
				"power": 1.5,
				"gravity": 0.08,
				"angle_offset": 0,
				"offset": [
					0,
					-0.1,
					0
				]
			},
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": true,
				"is_pushable_by_piston": true
			},
			"minecraft:environment_sensor": {
				"triggers": {
					"filters": {
						"test": "has_tag",
						"subject": "self",
						"operator": "equals",
						"value": "despawn"
					},
					"event": "endless:despawn"
				}
			}
		},
		"component_groups": {
			"endless:despawn": {
				"minecraft:explode": {
					"fuse_length": 0,
					"fuse_lit": true,
					"power": 1,
					"destroy_affected_by_griefing": true
				}
			},
			"endless:tick": {
				"minecraft:timer": {
					"time": [
						1,
						1
					],
					"looping": true,
					"time_down_event": {
						"event": "endless:tick_command"
					}
				}
			}
		},
		"events": {
			"endless:post_hit": {
				"sequence": [
					{
						"add": {
							"component_groups": [
								"endless:tick"
							]
						}
					},
					{
						"run_command": {
							"command": [
								"particle endless:cobalt_dynamite_smoke ~~~"
							]
						}
					}
				]
			},
			"endless:despawn": {
				"add": {
					"component_groups": [
						"endless:despawn"
					]
				}
			},
			"endless:tick_command": {
				"run_command": {
					"command": [
						"function EntityFunc/CobaltDynamite"
					]
				}
			}
		}
	}
}